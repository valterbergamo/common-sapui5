<mvc:View
    controllerName="xcop.fsc.service.controller.Home"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <Page
        showHeader="true"
        title="Exemplos de uso"
    >
        <l:BlockLayout background="Default"
        >
            <l:BlockLayoutRow>
                <l:BlockLayoutCell
                    backgroundColorSet="ColorSet9"
                    width="3"
                >
                   <IconTabBar>
                    <items>
                        <IconTabFilter>
                            <Input></Input>
                        </IconTabFilter>
                    </items>
                   </IconTabBar>
                </l:BlockLayoutCell>
            </l:BlockLayoutRow>
        </l:BlockLayout>
    </Page>
</mvc:View>
