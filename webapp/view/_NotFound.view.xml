<mvc:View
    controllerName="xcop.fsc.service.controller._NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <MessagePage
        title="{i18n>notFoundTitle}"
        text="{i18n>notFoundText}"
        icon="sap-icon://document"
        id="page"
        description="">
        <customDescription>
            <Link id="link" text="{i18n>backToHome}" press=".onLinkPressed"/>
        </customDescription>
    </MessagePage>

</mvc:View>